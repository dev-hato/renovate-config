{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "extends": [
    "config:recommended",
    ":timezone(Asia/Tokyo)"
  ],
  "automerge": true,
  "platformAutomerge": true,
  "prConcurrentLimit": 2,
  "configMigration": true,
  "packageRules": [
    {
      "matchPackageNames": [
        "npm"
      ],
      "enabled": false
    }
  ],
  "lockFileMaintenance": {
    "enabled": true
  }
}
